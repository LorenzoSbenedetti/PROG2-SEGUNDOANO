int vetorPonteiro(int *vet, int n){
    int i;
    for (i=0;i<=n;i++){
        scanf("%d",&vet[i]);
    }
    for (i=0;i<=n;i++){
        printf("%d\n",vet[i]);
    }
}
int vetorsemindice(int vet[], int n){
    int i;
    for (i=0;i<=n;i++){
        scanf("%d",&vet[i]);
    }
    for (i=0;i<=n;i++){
        printf("%d\n",vet[i]);
    }
}
int vetorindice(int vet[5]){
    int i;
    for (i=0;i<5;i++){
        scanf("%d",&vet[i]);

    }
    for (i=0;i<5;i++){
        printf("%d\n",vet[i]);
    }
}
int main()
{
    int n=5;
    int vet[n],i;
    printf("\nEscolha seu metodo para preencher o vetor :  ");
    scanf("%d",&i);
    switch(i){
        case 1:
        vetorPonteiro(vet,n);
        break;
        case 2:
        vetorsemindice(vet,n);
        break;
        case 3:
        vetorindice(vet);
        break;
    }
}
