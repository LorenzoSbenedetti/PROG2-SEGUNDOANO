#include <stdio.h>
#include <stdlib.h>

void carregar(int c[], int n){
    int i;
    for(i=0;i<n;i++){
        scanf("%d",&c[i]);
    }
}
void mostra(int c[], int n){
    int i;
    printf("\nOs valores dos vetores sÃ£o :\n");
    for(i=0;i<n; i++){
        printf("%d\n\n",c[i]);
    }
}
int main(){
    int k,n;
    scanf("%d",&n);
    int c[n];
    carregar(c,n);
    mostra(c,n);
}
