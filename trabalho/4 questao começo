#include <stdio.h>
#include <stdlib.h>
#include <math.h>


int recursiva(int x, int n){
    if (n==0){
       return x[n];
    }
    else {
        return x[recursiva(x[n-1],n);
    }
}
int main(){
    int n,i;
    scanf("%d",&n);
    int x[n];
    for (i=0;i<n;i++){
        scanf("%d",&x[i]);
    }
    printf("%d",recursiva("%d",x[n],n));
}
